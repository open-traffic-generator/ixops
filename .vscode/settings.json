{
    "editor.rulers": [
        88,
        90
    ],
    "go.useLanguageServer": true,
    "go.testEnvVars": {
        "CGO_ENABLED": 0,
    },
    "go.testFlags": [
        "-v",
        "-count",
        "1"
    ],
    "editor.formatOnSave": true,
    "gopls": {
        "build.env": {
            "CGO_ENABLED": 0,
        },
    },
    "go.toolsEnvVars": {
        "CGO_ENABLED": 0
    },
    "go.lintTool": "golangci-lint",
    "files.insertFinalNewline": true,
    "files.eol": "\n",
    "diffEditor.ignoreTrimWhitespace": false,
}
